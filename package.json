{
  "name": "documentation-theme-bespoke",
  "version": "0.3.5",
  "description": "A bespoke theme for documentation",
  "scripts": {
    "test": "xo && nyc ava",
    "coverage": "nyc ava && nyc report --reporter=lcov --report-dir test/coverage; open test/coverage/lcov-report/index.html"
  },
  "main": "index.js",
  "files": [
    "assets",
    "parts",
    "index.js"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/documentationjs/documentation-theme-default.git"
  },
  "keywords": [
    "documentation",
    "documentation-theme",
    "theme"
  ],
  "author": "Mark Griffiths",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/documentationjs/documentation-theme-default/issues"
  },
  "homepage": "https://github.com/documentationjs/documentation-theme-default",
  "dependencies": {
    "@thebespokepixel/badges": "^0.3.2",
    "anchor-js": "^4.1.0",
    "concat-stream": "^1.6.2",
    "documentation": "^8.0.0",
    "github-slugger": "^1.2.0",
    "highlight.js": "^9.12.0",
    "lodash": "^4.17.10",
    "remark": "^9.0.0",
    "remark-heading-gap": "^3.0.0",
    "remark-squeeze-paragraphs": "^3.0.1",
    "vinyl": "^2.2.0",
    "vinyl-fs": "^3.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.51",
    "@babel/preset-env": "^7.0.0-beta.51",
    "@thebespokepixel/oco-colorvalue-ex": "^0.3.0",
    "@thebespokepixel/palette2oco": "^0.2.0",
    "ace-css": "^1.1.0",
    "ava": "^0.25.0",
    "babel-plugin-lodash": "^3.3.4",
    "gulp": "^4.0.0",
    "gulp-better-rollup": "^3.2.1",
    "gulp-clean": "^0.4.0",
    "gulp-rename": "^1.3.0",
    "gulp-strip-comments": "^2.5.2",
    "gulp-stylus": "^2.7.0",
    "nib": "^1.1.2",
    "nyc": "^12.0.2",
    "rollup-plugin-babel": "^4.0.0-beta.5",
    "rollup-plugin-uglify": "^4.0.0",
    "shelljs": "^0.8.2",
    "typopro-web": "^3.7.9",
    "xo": "^0.21.1"
  },
  "xo": {
    "semicolon": false,
    "esnext": true,
    "ignores": [
      "index.js",
      "gulpfile.js",
      "assets/**/*.js"
    ]
  },
  "buildNumber": 0,
  "engines": {
    "node": ">=4.0 <9.0"
  }
}
